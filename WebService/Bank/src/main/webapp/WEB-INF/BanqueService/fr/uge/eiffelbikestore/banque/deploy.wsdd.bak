<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BanqueService WSDL service -->

  <service name="Banque" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://banque.eiffelbikestore.uge.fr"/>
      <parameter name="wsdlServiceElement" value="BanqueService"/>
      <parameter name="schemaQualified" value="http://banque.eiffelbikestore.uge.fr"/>
      <parameter name="wsdlServicePort" value="Banque"/>
      <parameter name="className" value="fr.uge.eiffelbikestore.banque.BanqueSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Banque"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getClientId" qname="operNS:getClientId" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" returnQName="retNS:getClientIdReturn" xmlns:retNS="http://banque.eiffelbikestore.uge.fr" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="nbCLient" qname="operNS:nbCLient" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" returnQName="retNS:nbCLientReturn" xmlns:retNS="http://banque.eiffelbikestore.uge.fr" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="addClient" qname="operNS:addClient" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getArgentClient" qname="operNS:getArgentClient" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" returnQName="retNS:getArgentClientReturn" xmlns:retNS="http://banque.eiffelbikestore.uge.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteClient" qname="operNS:deleteClient" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:client" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addMontant" qname="operNS:addMontant" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="isPossible" qname="operNS:isPossible" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" returnQName="retNS:isPossibleReturn" xmlns:retNS="http://banque.eiffelbikestore.uge.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="subMontant" qname="operNS:subMontant" xmlns:operNS="http://banque.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:montant" xmlns:pns="http://banque.eiffelbikestore.uge.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getClientId getArgentClient addClient deleteClient nbCLient isPossible subMontant addMontant"/>

  </service>
</deployment>
