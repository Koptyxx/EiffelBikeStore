<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ShopService WSDL service -->

  <service name="Shop" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://shop.eiffelbikestore.uge.fr"/>
      <parameter name="wsdlServiceElement" value="ShopService"/>
      <parameter name="schemaQualified" value="http://shop.eiffelbikestore.uge.fr"/>
      <parameter name="wsdlServicePort" value="Shop"/>
      <parameter name="className" value="fr.uge.eiffelbikestore.shop.ShopSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Shop"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getClient" qname="operNS:getClient" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:getClientReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:Client" xmlns:rtns="http://shop.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="cartValueConverted" qname="operNS:cartValueConverted" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:cartValueConvertedReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="loadBike" qname="operNS:loadBike" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" soapAction="" >
      </operation>
      <operation name="allClients" qname="operNS:allClients" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:allClientsReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:Client" xmlns:rtns="http://shop.eiffelbikestore.uge.fr" soapAction="" >
      </operation>
      <operation name="getDispo" qname="operNS:getDispo" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:getDispoReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="addToCart" qname="operNS:addToCart" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:addToCartReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:idBike" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="validBuy" qname="operNS:validBuy" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getBike" qname="operNS:getBike" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:getBikeReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:Bike" xmlns:rtns="http://shop.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="allBike" qname="operNS:allBike" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:allBikeReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:Bike" xmlns:rtns="http://shop.eiffelbikestore.uge.fr" soapAction="" >
      </operation>
      <operation name="registryClients" qname="operNS:registryClients" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:devise" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getBikePrice" qname="operNS:getBikePrice" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:getBikePriceReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getClientDevise" qname="operNS:getClientDevise" xmlns:operNS="http://shop.eiffelbikestore.uge.fr" returnQName="retNS:getClientDeviseReturn" xmlns:retNS="http://shop.eiffelbikestore.uge.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idClient" xmlns:pns="http://shop.eiffelbikestore.uge.fr" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="allClients validBuy registryClients getClientDevise getDispo addToCart getBike getClient cartValueConverted allBike loadBike getBikePrice"/>

      <typeMapping
        xmlns:ns="http://shop.eiffelbikestore.uge.fr"
        qname="ns:Client"
        type="java:fr.uge.eiffelbikestore.shop.Client"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://shop.eiffelbikestore.uge.fr"
        qname="ns:Bike"
        type="java:fr.uge.eiffelbikestore.shop.Bike"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
